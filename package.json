  "scripts": {
    "actions": "tsc && tslint --project tsconfig.json && npm run test:integration",
    "build": "npm run actions && node scripts/build.js",
    "build:prod": "npm run actions && node scripts/build.js --env=prod",
    "postbuild": "ts-node --skip-project config/postbuild.ts",
    "postbuild:prod": "npm run postbuild",
    "start": "npm run actions && node scripts/start.js",
    "start:prod": "node scripts/start.js --env=prod",
    "test": "node scripts/test.js --noWatch",
    "test:integration": "node scripts/test.js --config=config/jest/setupIntegration.js --noWatch --testPathPattern='.integration.test'",
    "test:unit": "node scripts/test.js --config=config/jest/setupUnit.js --watch --testPathIgnorePatterns='.integration.test'",
    "lint:fix": "tslint --fix -p tsconfig.json && eslint --fix ./src/",
    "format:code": "prettier --write src/**/*.{ts,tsx,js}"
  },
  "pre-push": [
    "actions"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 9",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "react-native$": "react-native-web",
      ".+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "modulePaths": [
      "<rootDir>/src"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setupFilesAfterEnv.js"
    ],
    "snapshotResolver": "<rootDir>/config/jest/snapshotResolver.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      ".+\\.(js|jsx|ts|tsx)$": "babel-jest",
      ".+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      ".+\\.json$": "<rootDir>/config/jest/jsonTransform.js",
      "(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      ".+\\.module\\.(css|sass|scss)$"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
